
schema {
    query: Query
}

type Query {
    getAllUsers:[User]
}

type Mutation {
   addUser(firstName: String!, lastName: String!, address: String): User!
}

type User {
    userId : ID!,
    firstName :String,
    lastName :String,
    address:String,
    post: [Post],
}

type Post {
    postId: ID!,
    postName : String,
}