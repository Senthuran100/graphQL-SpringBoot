
schema {
    query: Query
}

type Query {
    getAllUsers:[User]
    getUserById(userId: ID): User
}

type Mutation {
   addUser(firstName: String!, lastName: String!, address: String): User!
   addPost(postName:String!,userId: ID!): Post
}

type User {
    userId : ID!,
    firstName :String,
    lastName :String,
    address:String,
    post: [Post],
}

type Post {
    postId: ID!,
    postName : String,
}